# Production Environment Variables for EarthGazer
# Copy this file to .env.prod and fill in your values

# Database Configuration
DB_USER=earthgazer
DB_PASSWORD=changeme_secure_password
DB_NAME=earthgazer_prod

# Redis Configuration
REDIS_PASSWORD=changeme_redis_password

# Google Cloud Configuration
# Base64-encoded service account JSON
GCLOUD_SERVICE_ACCOUNT=
GCLOUD_BUCKET_NAME=your-earthgazer-bucket

# Flower Monitoring UI
FLOWER_USER=admin
FLOWER_PASSWORD=changeme_flower_password

# Optional: Celery Configuration Overrides
# EARTHGAZER__CELERY__WORKER_CONCURRENCY=4
# EARTHGAZER__CELERY__TASK_ACKS_LATE=true
# EARTHGAZER__CELERY__RESULT_EXPIRES=3600
